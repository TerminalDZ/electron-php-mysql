{
  "name": "electron-php-mysql",
  "version": "2.0.0",
  "description": "An easy-to-use Electron application that integrates PHP and MySQL, providing a seamless development environment for desktop applications with web technologies.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --no-asar --platform=win32 --arch=ia32 --icon=icons/logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\""
  },
  "repository": "https://github.com/TerminalDZ/electron-php-mysql",
  "keywords": [
    "Electron",
    "php",
    "techsoul"
  ],
  "author": "Idriss Boukmouche",
  "devDependencies": {
    "electron": "^31.3.1",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "path": "^0.12.7",
    "php-server-manager": "^1.3.3"
  }
}
