{
  "name": "TerminalDZ",
  "version": "1.0.0",
  "description": "An easiest way to use php in electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --no-asar --icon=icons/logo.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --no-asar --platform=win32 --arch=ia32 --icon=icons/logo.ico --prune=true --out=release-builds --version-string.CompanyName=IBA --version-string.FileDescription=IBA --version-string.ProductName=\"Terminaldz\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --no-asar --platform=linux --arch=x64 --icon=icons/logo.png --prune=true --out=release-builds"
  },
  "author": "idriss Boukmouche",
  "devDependencies": {
    "electron": "^22.3.25",
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "dotenv": "^16.3.1",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "php-server-manager": "^1.3.3"
  }
}
